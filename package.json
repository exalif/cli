{
  "name": "exalif-cli",
  "version": "3.2.0",
  "description": "Exalif CLI to deploy on Rancher 2 and do other magic stuff",
  "main": "app.js",
  "scripts": {
    "test": "npx nyc mocha './{,!(node_modules)/**/}*.spec.js'",
    "test:watch": "npx mocha -w './{,!(node_modules)/**/}*.spec.js'",
    "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "private": false,
  "author": "exalif",
  "license": "MIT",
  "dependencies": {
    "ansi-styles": "4.2.1",
    "child-process-promise": "2.2.1",
    "dotenv": "8.2.0",
    "figlet": "1.3.0",
    "js-yaml": "3.13.1",
    "ora": "4.0.3",
    "request": "2.88.0",
    "request-promise-native": "1.0.8",
    "uuid": "7.0.2",
    "yargs": "15.3.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "conventional-changelog-cli": "2.0.31",
    "coveralls": "3.0.9",
    "mocha": "7.1.0",
    "nyc": "15.0.0",
    "proxyquire": "2.1.3",
    "sinon": "9.0.1",
    "sinon-chai": "3.5.0"
  },
  "nyc": {
    "all": true,
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ],
    "reporter": [
      "text",
      "text-summary",
      "lcov"
    ]
  },
  "resolutions": {
    "request": "2.88.0"
  }
}
